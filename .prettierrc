{
  "tabWidth": 3,
  "semi": false,
  "arrowParens": "always",
  "printWidth": 100,
  "trailingComma": "es5",
  "singleQuote": true,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "strict",
  "insertPragma": false,
  "proseWrap": "always",
  "quoteProps": "consistent",
  "requirePragma": false,
  "useTabs": false,
  "overrides": [
    {
      "files": "*.component.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
